{  
  "map_enabled": true,
  "auto_pin_capturing_locations": false,
  "auto_unpin_on_clear": true,
  "allow_resize": false,
  "layout_swap_left_right": false,
  "disabled_image_filter": "grayscale, dim",
  "layouts": {
    "shared_item_grid": {
      "type": "array",
      "h_alignment": "horizontal",
      "content": [
        {
					"type": "array",
					"orientation": "horizontal",
					"margin": "0,5,0,0",
					"content": [
						{
							"type": "array",
							"orientation": "horizontal",
							"margin": "-5,-5,0,0",
							"content": [
								{
									"type": "itemgrid",
									"h_alignment": "center",
									"item_margin": "1,2",
									"rows": [
										[ "earthorb", "fireorb", "waterorb", "airorb" ],
										[ "bridge", "lute", "crown", "crystal", "herb"],
										[ "key", "tnt", "canal", "ruby","rod" ],
										[ "canoe", "floater", "tail", "bottle","oxyale"],
                    [ "slab", "chime", "cube", "adamant","masmune"]
									]
								}
							]
						},
						{
              "type": "array",
              "orientation": "horizontal",
              "margin": "-5,-10,0,0",
              "content": [
                //LINHA
                { 
                  "type": "itemgrid",
                  "h_alignment": "center",
                  "item_margin": "0,0",
                  "rows": [
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ],
                    [ "linhaa" ]
                  ]
                }
              ]
            },
						{
							"type": "array",
							"orientation": "horizontal",
							"margin": "-35,-5,0,0",
							"content": [
								{
									"type": "itemgrid",
									"h_alignment": "center",
									"item_margin": "1,2",
									"rows": [
										["garland", "king", "sara", "bikke", "marsh"],
										["marshLocked", "astos", "matoya", "prince", "coneriaLocked"],
										["nerrick", "smith", "earth", "unne", "sarda"],
										["sage", "volcano", "iceCave", "ordeals", "fairy"],
										["seaShrine", "waterfall", "lefein", "skyPalace"]
									]
								}
							]
						}
					]
				}
      ]
    },
    "tracker_capture_item": {
      "type": "container",
      "content": {
        "type": "layout",
        "key": "shared_item_grid"
      }
    },
    "tracker_default": {
      "type": "container",
      "width": 365,
      "height": 200,
      "background": "#212121",
      "content": {
        "dropshadow": true,
        "margin": 5,
        "type": "layout",
        "key": "shared_item_grid"
      }
    }
  }
}